<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bimo.syahputro.chromeinc">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activity.detailTransaksi.DetailTransaksiActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.daftarTransaksi.DaftarTransaksiActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.dashboard.DashboardActivity"
            android:theme="@style/MyTheme2" />
        <activity android:name=".activity.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>